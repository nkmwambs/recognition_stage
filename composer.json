{
    "name": "compassion/recognition",
    "description": "This is a staff nomination project used across the Africa Region to nominate the best staff of the year based on HR defined categories",
    "type": "project",
    "require": {
        "simplesamlphp/simplesamlphp": "^1.17"
    },
    "authors": [
        {
            "name": "nkmwambs",
            "email": "nkmwambs@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "php": ">=5.6",
        "phpunit/phpunit": "^5.0",
        "satooshi/php-coveralls": "^1.0"
    },
   "autoload": {
        "psr-4": {
            "Base32\\": "application/libraries"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Base32\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
